// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Club {

  id String @id @default(uuid())

  name       String
  source_url String
  logo_url   String
  location   String @default(value: "Brasil")
  @@map("clubs")
}
